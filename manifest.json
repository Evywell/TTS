{
  "manifest_version": 2,
  "name": "Application Certification G Suite",
  "description": "Lien vers le site Web Certification G Suite",
  "version": "1.1",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["src/responsivevoice.js", "src/app.js"]
  },
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}